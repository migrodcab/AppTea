<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.hp.apptea.MainActivity">

    <com.google.android.gms.common.SignInButton
        android:id="@+id/sign_in_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SÃ­mbolos"
        android:id="@+id/simbolos"
        android:layout_centerHorizontal="true"
        android:layout_gravity="top"
        android:onClick="cargaMenuSimbolos" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Texto"
        android:id="@+id/texto"
        android:layout_marginTop="62dp"
        android:layout_below="@+id/simbolos"
        android:layout_centerHorizontal="true"
        android:onClick="cargaMenuTexto" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Prueba"
        android:id="@+id/prueba"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/simbolos"
        android:layout_alignStart="@+id/simbolos"
        android:onClick="cargaMenuPrueba" />

</RelativeLayout>
